  "private": true,
  "author": "GS&F Devs <mindreactor@gsandf.com> (https://gsandf.com/)",
  "license": "UNLICENSED",
  "ava": {
    "extensions": [
      "js",
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "ignoredByWatcher": [
      "!src/**"
    ],
    "require": [
      "./.ava-entry.js"
    ]
  },
  "dependencies": {
    "@blakek/deep": "^2.2.0",
    "@gsandf/ui": "^0.1.2",
    "next": "^11.0.1",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/styled-components": "^5.1.10",
    "amper-scripts": "^1.2.3",
    "ava": "^3.15.0",
    "babel-plugin-styled-components": "^1.13.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format-check": "amper-scripts format-check .",
    "format": "amper-scripts format-write .",
    "lint": "amper-scripts lint .",
    "start": "next start",
    "test": "ava",
    "typeCheck": "tsc --noEmit",
    "validate": "run-p format-check lint typeCheck"
  }
}
