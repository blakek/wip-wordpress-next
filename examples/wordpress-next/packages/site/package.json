  "ava": {
    "extensions": [
      "js",
      "ts"
    ],
    "files": [
      "./**/*.test.ts"
    ],
    "ignoredByWatcher": [
      "!src/**"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "dependencies": {
    "@blakek/array-split": "^2.0.1",
    "@blakek/deep": "^2.1.1",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/styled-components": "^5.1.7",
    "axios": "^0.21.1",
    "date-fns": "^2.18.0",
    "next": "^10.0.7",
    "polished": "^4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "unchanged": "^2.2.1"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "format-check": "prettier --check **/*.{js,ts,tsx}",
    "lint": "eslint --config ../../.eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "start": "next start",
    "test": "ava",
    "typeCheck": "tsc --noEmit",
    "validate": "run-p format-check lint typeCheck"
  },
  "devDependencies": {
    "amper-scripts": "^1.0.0",
    "ava": "^3.15.0",
    "babel-plugin-styled-components": "^1.11.1",
