{
  "name": "@gsandf/site-root",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "author": "GS&F Devs <mindreactor@gsandf.com> (https://gsandf.com/)",
  "license": "MIT",
  "devDependencies": {
    "amper-scripts": "^1.0.0-0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1"
  },
  "scripts": {
    "format-check": "run-p 'create-site format-check' 'wordpress-graphql-schema format-check'",
    "format": "run-p 'create-site format-write' 'wordpress-graphql-schema format-write'",
    "lint": "run-p 'create-site lint' 'wordpress-graphql-schema lint'",
    "test": "run-p 'wordpress-graphql-schema test'",
    "create-site": "yarn workspace @gsandf/create-site",
    "wordpress-graphql-schema": "yarn workspace @gsandf/wordpress-graphql-schema"
  },
  "workspaces": [
    "packages/*"
